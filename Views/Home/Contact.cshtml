        <label>Telefone</label>
        <input type="tel" class="form-control" />
    </div>
    <div class="form-group">
        <label>Número</label>
        <input type="number" class="form-control" />
    </div>
    <button class="btn btn-primary">Enviar</button>
</form>

<address>
